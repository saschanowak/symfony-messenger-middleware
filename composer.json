{
    "name": "tideways/symfony-messenger-middleware",
    "description": "Provides a Middleware to integration Tideways into Symfony Messenger Processing",
    "license": "mit",
    "require": {
        "php": "^7.2|^8.0",
        "symfony/messenger": "^4.3 || ^5.0 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Tideways\\SymfonyMessenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tideways\\Tests\\SymfonyMessenger\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    }
}
